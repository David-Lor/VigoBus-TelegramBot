# Set your bot Token generated from @BotFather
TOKEN=

API_TIMEOUT=10
PERSIST_TIMEOUT=1.5
SKIP_PREV_UPDATES=1

USER_RATE_LIMIT_AMOUNT=5
USER_RATE_LIMIT_TIME=2


# IMPORTANT: the STATIC_PATH must be set, cannot be autodetected when deployed on Docker
# The STATIC_PATH must match the APP_NAME
APP_NAME=VigoBusBot
STATIC_PATH=/home/appuser/VigoBusBot/static

LOG_LEVEL=debug
